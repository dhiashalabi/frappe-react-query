{
    "printWidth": 120,
    "tabWidth": 4,
    "useTabs": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "semi": false,
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "jsxSingleQuote": false,
    "quoteProps": "as-needed",
    "embeddedLanguageFormatting": "auto",
    "singleAttributePerLine": true,
    "endOfLine": "auto"
}
